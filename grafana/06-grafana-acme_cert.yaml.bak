apiVersion: cert-manager.io/v1
kind: Certificate
metadata:
  name: grafana-certs
  namespace: grafana
  labels:
    app.kubernetes.io/component: grafana
    app.kubernetes.io/part-of: monitoring-system
spec:
  secretName: grafana-tls
  isCA: false
  issuerRef:
    group: cert-manager.io
    kind: ClusterIssuer
    name: letsencrypt-prd
  privateKey:
    algorithm: ECDSA
    size: 256
  dnsNames:
  - nms.jakwifi.id
---